{
  "name": "@rlxyz/compiler",
  "common": {
    "ethereum": "rlxyz.eth"
  },
  "engines": {
    "node": "16.16.0"
  },
  "version": "1.0.1",
  "description": "A library based on Boolean Logic for generative art compilation",
  "author": "RLXYZ",
  "contributors": [
    {
      "name": "Jeevan Pillay",
      "email": "jeevan@rhapsodylabs.xyz",
      "url": "https://twitter.com/jeevanpillay"
    }
  ],
  "license": "MIT",
  "main": "./index.ts",
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "src/**/*"
    ]
  },
  "keywords": [
    "nft",
    "generative",
    "genart",
    "canvas"
  ],
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "tsc",
    "dev:gen": "ts-node-dev src/generate.ts",
    "dev:api": "ts-node-dev src/index.ts",
    "watch:api": "NODE_ENV=development ts-node-dev index.ts",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "start": "ts-node index.ts",
    "prepare": "npm run build",
    "publish": "npm publish --access public",
    "postpublish": "PACKAGE_VERSION=$(./scripts/package-version.sh) && git tag -ae v$PACKAGE_VERSION && git push --tags",
    "version": "npm run format && git add ."
  },
  "dependencies": {
    "@metamask/eth-sig-util": "^4.0.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "canvas": "^2.8.0",
    "canvas-sketch": "^0.7.5",
    "canvas-sketch-util": "^1.10.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.1",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.6.9",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "jest": "^27.0.3",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.2"
  }
}
