{
  "name": "@rhapsodylabs/rhapsodys",
  "common": {
    "ethereum": "donations.rhapsodylabs.eth"
  },
  "version": "1.0.0",
  "description": "A generative art compiler for NFT projects",
  "author": "Rhapsody Labs",
  "contributors": [
    {
      "name": "Jeevan Pillay <jeevan.rhapsodylabs.eth>",
      "email": "jeevan@rhapsodylabs.xyz",
      "url": "https://twitter.com/jeevanpillay"
    }
  ],
  "license": "MIT",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "src/**/*"
    ]
  },
  "keywords": [
    "nft",
    "generative",
    "genart",
    "canvas"
  ],
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "tsc",
    "start": "yarn build && node build/index.js",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "publish": "npm publish --access public",
    "postpublish": "PACKAGE_VERSION=$(./scripts/package-version.sh) && git tag -ae v$PACKAGE_VERSION && git push --tags",
    "version": "npm run format && git add ."
  },
  "dependencies": {
    "canvas": "^2.8.0",
    "crypto-js": "^4.1.1",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "jest": "^27.0.3",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.2"
  }
}
